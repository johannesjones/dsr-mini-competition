{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Kedro Run",
            "type": "debugpy",
            "request": "launch",
            "module": "kedro",
            "args": ["run"],
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "name": "Python: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        }
    ]
}
